{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://warrentymfrontend.netlify.app",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}